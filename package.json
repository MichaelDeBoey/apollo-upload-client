    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "gzip-size": "^7.0.0",
    "prettier": "^3.6.2",
    "revertable-globals": "^4.0.0",
    "rxjs": "^7.8.2",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "prettier": "prettier -c .",
    "eslint": "eslint",
    "types": "tsc -p jsconfig.json",
    "test": "coverage-node --test-reporter=spec --test",
    "check": "node --run prettier && node --run eslint && node --run types && node --run test",
    "prepublishOnly": "node --run check"
  }
}
